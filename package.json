{
  "name": "scrapbox-parser",
  "version": "1.0.0",
  "description": "parse Scrapbox notation to JavaScript Object",
  "main": "src/index.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p ./tsconfig.json",
    "predoc": "rm -rf docs",
    "doc": "typedoc --out docs ./src",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/progfay/scrapbox-parser.git"
  },
  "keywords": [
    "scrapbox",
    "parser"
  ],
  "author": "progfay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/progfay/scrapbox-parser/issues"
  },
  "homepage": "https://github.com/progfay/scrapbox-parser#readme",
  "devDependencies": {
    "@types/core-js": "^2.5.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {},
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  }
}
